{
    "name": "cli",
    "version": "0.3.3",
    "description": "Decouple deploy from release in seconds.",
    "keywords": [
        "cli",
        "keat",
        "feature flags",
        "feature toggle",
        "feature management",
        "continuous delivery",
        "release"
    ],
    "license": "MIT",
    "author": "wito.delnat@gmail.com",
    "sideEffects": false,
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/config.d.ts",
            "module": "./dist/config.js",
            "require": "./dist/config.js",
            "default": "./dist/config.js"
        }
    },
    "source": "src/config.ts",
    "types": "./dist/config.d.ts",
    "bin": {
        "keat": "./dist/cli.cjs"
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "typecheck": "tsc --noEmit",
        "build": "npm-run-all build:*",
        "build:clean": "rimraf dist",
        "build:cli": "esbuild src/cli/main.cts --bundle --platform=node --outfile=dist/cli.cjs --external:esbuild --external:typescript --external:prettier --tsconfig=tsconfig.cli.json",
        "build:config": "tsc --project tsconfig.config.json",
        "codegen": "buf generate ../keat-release-api/proto/keat/core/v1/core.proto"
    },
    "dependencies": {
        "@bufbuild/protobuf": "1.10.0",
        "@connectrpc/connect": "1.6.1",
        "@connectrpc/connect-web": "1.6.1",
        "@dotenvx/dotenvx": "1.20.0",
        "esbuild": "0.14.53",
        "node-fetch": "3.3.2"
    },
    "peerDependencies": {
        "prettier": "^2.0",
        "typescript": "^4.0 || ^5.0"
    },
    "peerDependenciesMeta": {
        "prettier": {
            "optional": true
        }
    },
    "devDependencies": {
        "@bufbuild/buf": "1.45.0",
        "@bufbuild/protoc-gen-es": "1.10.0",
        "@connectrpc/protoc-gen-connect-es": "1.6.1",
        "@types/diff": "5.0.2",
        "@types/fs-extra": "9.0.13",
        "@types/json-diff": "0.9.0",
        "@types/klaw-sync": "6.0.1",
        "@types/lodash": "4.14.184",
        "@types/node": "18.6.1",
        "@types/node-fetch": "2.6.11",
        "@types/prettier": "2.7.1",
        "@types/prompts": "2.0.14",
        "@types/react": "18.0.21",
        "@types/yargs": "17.0.10",
        "boxen": "5.1.2",
        "chalk": "4.1.2",
        "change-case": "4.1.2",
        "chrono-node": "2.4.1",
        "date-fns": "2.29.3",
        "diff": "5.1.0",
        "es6-error": "4.1.1",
        "fs-extra": "10.1.0",
        "json-diff": "0.9.1",
        "jwt-decode": "3.1.2",
        "keat": "1.0.0-rc.5",
        "klaw-sync": "6.0.0",
        "lodash": "4.17.21",
        "log-symbols": "4.1.0",
        "mri": "1.2.0",
        "npm-run-all": "4.1.5",
        "open": "8.4.0",
        "prettier": "2.7.1",
        "prompts": "2.4.2",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "rimraf": "3.0.2",
        "strands": "1.0.1",
        "tiny-invariant": "1.2.0",
        "ts-morph": "16.0.0",
        "type-fest": "2.18.0",
        "typescript": "5.6.3",
        "untildify": "4.0.0",
        "xdg-app-paths": "8.2.0"
    }
}
