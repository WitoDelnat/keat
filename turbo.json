{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["lib"]
        },
        "typecheck": {
            "dependsOn": ["^typecheck"]
        },
        "dev": {
            "persistent": true,
            "cache": false
        }
    }
}
